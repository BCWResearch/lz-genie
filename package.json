{
  "name": "layerzero-quickstart",
  "version": "1.0.0",
  "description": "A set of boilerplate, scaffolding and ready to use tools to get a dev up and running quickly on l0 usecases",
  "submodules": [
    "solidity-examples"
  ],
  "main": "bin/src/index.js",
  "bin": {
    "lzgenie": "bin/src/index.js"
  },
  "scripts": {
    "postinstall": "submodules-install",
    "start": "ts-node src/index.ts",
    "build": "rm -rf bin && tsc && chmod -R 755 bin && cp -r solidity-examples bin/solidity-examples && cp -r projects bin/projects",
    "install:local": "yarn build && npm install -g",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@inquirer/prompts": "^4.3.0",
    "cli-progress": "^3.12.0",
    "inquirer": "^9.2.16",
    "inquirer-table-prompt": "^0.2.1",
    "nodemon": "^3.1.0",
    "progress": "^2.0.3",
    "require-from-string": "^2.0.2",
    "submodules-install": "^1.1.0",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.5",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.11.30",
    "typescript": "^5.4.3"
  }
}
